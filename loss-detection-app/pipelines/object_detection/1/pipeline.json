{
	"type": "GStreamer",
	"template":["urisourcebin name=\"source\" ! queue ! decodebin ! videoconvert name=\"videoconvert\" ! video/x-raw",
		" ! videocrop name=\"videocrop\" ! queue leaky=upstream",
		" ! gvadetect device=CPU model=\"{models[object_detection][1][network]}\" model-proc=\"{models[object_detection][1][proc]}\" name=\"detection\"",
		" ! gvametaconvert name=\"metaconvert\" add-empty-results=true",
		" ! queue",
		" ! gvametapublish name=\"destination\"",
		" ! queue",
		" ! appsink name=appsink"
		],
	"description": "Object Detection Pipeline",

	"parameters": {
		"type" : "object",
		"properties" : {
			"inference-interval": {
				"element":"detection",
				"type": "integer",
				"minimum": 0,
				"maximum": 4294967295,
				"default": 1
			},
			"n-threads": {
				"element":"videoconvert",
				"type": "integer",
				"default": 1
			},
			"nireq": {
				"element":"detection",
				"type": "integer",
				"minimum": 1,
				"maximum": 64,
				"default": 2
			},
			"top": {
				"element": "videocrop",
				"type": "integer",
				"default": 0
			},
			"bottom": {
				"element": "videocrop",
				"type": "integer",
				"default": 0
			},
			"left": {
				"element": "videocrop",
				"type": "integer",
				"default": 0
			},
			"right": {
				"element": "videocrop",
				"type": "integer",
				"default": 0
			}
		}
	}
}
